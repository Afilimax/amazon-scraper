{
  "name": "@afilimax/amazon-scraper",
  "version": "0.1.0",
  "type": "commonjs",
  "description": "Afilimax Amazon Scraper é um pacote de extração de dados do marketplace da Amazon, baseado do framework de Web Scraping \"Xcrap\"",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/*",
    "!/**/__tests__"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "ts-node ./src/index.ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "test": "jest"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "afilimax",
    "amazon scraper",
    "brazilian",
    "product extractor"
  ],
  "author": "Marcuth",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^25.2.3",
    "eslint": "^10.0.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.5",
    "jest": "^30.2.0",
    "prettier": "^3.8.1",
    "prettier-plugin-sort-imports": "^1.8.11",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@mihnea.dev/puppeteer-extra-amazon-captcha": "^1.0.3",
    "@xcrap/core": "^0.2.3",
    "@xcrap/extractor": "^0.3.2",
    "@xcrap/got-scraping-client": "^0.1.0",
    "@xcrap/puppeteer-extra-client": "^0.1.0",
    "@xcrap/transformer": "^0.3.7",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "winston": "^3.19.0"
  },
  "peerDependencies": {
    "@afilimax/core": "^0.2.2"
  }
}
